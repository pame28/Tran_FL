using transacciones_flotantes.Infrastructure.Data;
using transacciones_flotantes.Models.Validations;
using transacciones_flotantes.Repository.Interface;

namespace transacciones_flotantes.Repository
{
    public class ParametrosRepositoryImp : IParametrosRepository
    {
        private readonly DBHelper _dbHelper;
        public ParametrosRepositoryImp(DBHelper dbHelper)
        {
            _dbHelper = dbHelper;
        }

        public async Task<Parametros> GetByCode(string clave)
        {
            try
            {
                using var reader = await _dbHelper.ExecuteReaderAsync("getParametroByClave",
                    new Dictionary<string, object> { { "@Clave", clave } });

                while (await reader.ReadAsync())
                {
                    return new Parametros
                    {
                        clave = reader.GetString(0),
                        valor = reader.GetString(1)
                    };  
                }
                return new Parametros();
            }
            catch (Exception ex)
            {
                throw new Exception($"{ex.Message}");
            }
        }
    }
}




