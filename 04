namespace transacciones_flotantes.Models.TransaccionesFlotantes
{
    public class TransaccionFlotanteProveedorDataModel
    {
        public string BinTarjeta {  get; set; }
        public string UltimosCuatroDigitos { get; set; }
        public string NoDocumento { get; set; }
        public string TipoDocumento { get; set; }
        public string Sexo { get;set; }

    }
}




